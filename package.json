{
  "name": "discord-ai-sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "A sdk made to allow ai to interact with discord api for discord bots",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rubr1c/discord-ai-sdk.git"
  },
  "homepage": "https://github.com/Rubr1c/discord-ai-sdk#readme",
  "bugs": {
    "url": "https://github.com/Rubr1c/discord-ai-sdk/issues"
  },
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.17 || >=20"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "vitest -c vitest.config.ts",
    "test:run": "vitest run -c vitest.config.ts",
    "docs": "typedoc",
    "prepublishOnly": "pnpm typecheck && pnpm lint && pnpm build"
  },
  "keywords": [
    "discord",
    "discord.js",
    "ai",
    "vercel-ai-sdk",
    "llm",
    "sdk",
    "middleware",
    "bot",
    "tools",
    "slash-commands",
    "typescript"
  ],
  "packageManager": "pnpm@10.13.1",
  "peerDependencies": {
    "ai": "^5.0.0",
    "discord.js": "^14.0.0"
  },
  "dependencies": {
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "ai": "^5.0.22",
    "discord.js": "^14.22.1",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "tsup": "^8.5.0",
    "typedoc": "^0.26.7",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
